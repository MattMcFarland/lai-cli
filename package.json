{
  "author": "mattmcfarland @MattMcFarland",
  "bin": {
    "lai": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3.16.0",
    "@oclif/plugin-help": "^5.2.20",
    "@oclif/plugin-plugins": "^4.1.14",
    "@types/dotenv": "^8.2.0",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/joi": "^17.2.3",
    "axios": "^1.6.5",
    "didyoumean2": "^6.0.1",
    "dotenv": "^16.3.1",
    "fast-levenshtein": "^3.0.0",
    "inquirer": "^9.2.12",
    "joi": "^17.11.0",
    "lunr": "^2.3.9",
    "uuid": "^9.0.1"
  },
  "description": "Local AI CLI Assistance and Tools",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.1.8",
    "@types/chai": "^4.3.11",
    "@types/inquirer": "^9",
    "@types/lunr": "^2.3.7",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.4",
    "@types/uuid": "^9",
    "chai": "^4.4.0",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5.0.0",
    "eslint-config-oclif-typescript": "^3.0.37",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.2.0",
    "oclif": "^4.1.4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/MattMcFarland/lai-cli",
  "license": "MIT",
  "main": "",
  "name": "lai-cli",
  "oclif": {
    "flexibleTaxonomy": true,
    "bin": "lai",
    "dirname": "lai",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " ",
    "hooks": {
      "init": "./dist/hooks/init/add-env-vars"
    }
  },
  "repository": "MattMcFarland/lai-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "1.0.0",
  "bugs": "https://github.com/MattMcFarland/lai-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
